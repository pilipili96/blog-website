<%- include("partials/header")-%>

<h1>Home</h1>
<p><%= homeStartingContent %></p>
<% posts.forEach(function(post){%>
    <h1><%= post.title %></h1>
    <% if(post.content.length < 100){%>
        <p><%=post.content %></p>
    <%} else {%>
    <p><%= post.content.slice(0,100) + " ... " %><a href="posts/<%=post.title%> ">Read More</a> </p>
<%}});%>


